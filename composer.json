{
    "name": "lsv/timestringparser",
    "license": "MIT",
    "description": "Parse a text string to minutes in integer",
    "keywords": ["Minutes", "Text", "Parse"],
    "require": {
        "php": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lsv\\TimestringParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lsv\\TimestringParserTest\\": "tests"
        }
    },
    "require-dev": {
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^3.0",
        "phploc/phploc": "^3.0",
        "phing/phing": "^2.16",
        "squizlabs/php_codesniffer": "^2.8",
        "friendsofphp/php-cs-fixer": "^2.2",
        "phpunit/phpunit": "^6.0"
    }
}
